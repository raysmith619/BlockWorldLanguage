// test3.bwif - simple display with variables
set winh = 900;
set winw = 1400;
add window size=winw,winh;
set lex=10;
set ley=lex;
set lez=20;
add lookatEye loc=lex,ley,lez;
add lookatcenter loc=0;
add lookatup loc=0,1,0;
add block color=1,0,0 loc=-12,-2,-2 size=1,2,3;
add sphere color=0,0,1 loc=4.5,5,2 size=2;
add cone color=0,1,0 loc=5,5,6 size=1,4;
add cylinder color=0,1,1 loc=6,2,3 size=1,2,3;
display;
