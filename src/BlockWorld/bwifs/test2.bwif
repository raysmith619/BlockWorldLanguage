// Include variables
set height = 900;
set wid = 800;
add window size=height,wid;
set leyex = 10;
set leyey = leyex;
set leyez = leyex;
add lookatEye loc=1,2,3;
add lookatEye loc=leyex,leyey,leyez;
add lookatcenter loc=0;
add lookatup loc=0,1,0;
set blockSizeX = 1;
set blockSizeY = 2;
set blockSizeZ = 3;
add block color=1,0,0 loc=-12,-2,-2
	 size=blockSizeX,blockSizeY,blockSizeZ;
add sphere color=0,0,1 loc=4.5,5,2 size=2;
add cone color=0,1,0 loc=5,5,6 size=1,4;
add cylinder color=0,1,1 loc=6,2,3 size=1,2,3;
display;
